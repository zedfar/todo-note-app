    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "react-refresh": "^0.18.0",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.2"

    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.546.0",
    "metro-config": "^0.83.3",
    "nativewind": "^4.0.36",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.0",
    "react-native": "0.81.4",
    "react-native-dotenv": "^3.4.11",
